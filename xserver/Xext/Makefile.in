	$(LINK) $(am_libXextbuiltin_la_rpath) $(libXextbuiltin_la_LDFLAGS) $(libXextbuiltin_la_OBJECTS) $(libXextbuiltin_la_LIBADD) $(LIBS)
libXextmodule.la: $(libXextmodule_la_OBJECTS) $(libXextmodule_la_DEPENDENCIES) 
